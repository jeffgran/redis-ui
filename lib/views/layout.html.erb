<html>
<head>
	<title>RedisUI</title>
	<link rel="stylesheet" href="/stylesheets/redis-ui.css" type="text/css" media="screen" charset="utf-8">
</head>
<body>
	<h1>RedisUI</h1>
	<div id="main"></div>
	<%= yield %>
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.6/jquery.js"></script>
	<script src="/js/lib/underscore.js"></script>
